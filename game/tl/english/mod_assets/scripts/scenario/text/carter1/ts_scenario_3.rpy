    # ts_ft 2b "Я проголодался, просто умираю с голоду!"
    ts_ft 2b "I'm hungry, I'm just starving!"

# game/mod_assets/scripts/scenario/text/carter1/ts_scenario_3.rpy:2107
translate english ts_scenario_3_d83642dd:

    # "Не ты один..."
    "You're not the only one..."

