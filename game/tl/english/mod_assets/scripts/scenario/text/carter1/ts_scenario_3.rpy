    # ts_ft "И с чем?"
    ts_ft "Tea with what?"

# game/mod_assets/scripts/scenario/text/carter1/ts_scenario_3.rpy:2117
translate english ts_scenario_3_340aae72:

    # m "С ложкой..."
    m "With a spoon..."

# game/mod_assets/scripts/scenario/text/carter1/ts_scenario_3.rpy:2118
